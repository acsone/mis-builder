<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="mis_account_move_line_tree_view">
        <field name="name">mis.account.move.line.tree</field>
        <field name="model">mis.account.move.line</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date" />
                <field name="name" />
                <field name="account_id" />
                <field
                    name="analytic_account_id"
                    groups="analytic.group_analytic_accounting"
                />
                <field name="debit" />
                <field name="credit" />
            </tree>
        </field>
    </record>
</odoo>
